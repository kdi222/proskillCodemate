{{=<% %>=}}

<style>
    .levelbackground{
      background:url('data:image/png;base64,');
      background-repeat: no-repeat;
      background-size: 35px 35px;
      width:35px;
      height:35px;
      padding-top:10px;
    }
    .accordion {
        background-color: #eee;
        color: #444;
        cursor: pointer;
        padding: 10px;
        width: 100%;
        border: none;
        text-align: left;
        outline: none;
        font-size: 15px;
        transition: 0.4s;
      }

      .active, .accordion:hover {
        background-color: #ccc;
      }

      .accordion:after {
        content: '\002B';
        color: #777;
        font-weight: bold;
        float: right;
        margin-left: 5px;
      }

      .active:after {
        content: "\2212";
      }

      .panel {
        padding: 0 18px;
        background-color: white;
        max-height: 0;
        overflow: hidden;
        transition: max-height 0.2s ease-out;
      }

    .card-title {
        width: 100%;
        height: 35px;
        display: block!important;
        border-left: 10px solid #f28a3c;
        border-right: 40px solid #f28a3c;
        background: #e3e2e2;
        padding-top: 7px;
        padding-left: 10px;
        font-size: 15px;
        color: #000;
        margin-top:0px !important;
    }
    
    .card-text{
        width:100%; 
        min-height:150px; 
        background:white; 
        padding:0px;
        margin-top: -10px;
    }
    .card-body{
        position: relative;

    }
    .expand-collapse-button{
        font-size:25px;
        color:white;
        position:absolute;
        top:0px; 
        right:0px;
        display:block;
        padding:0 10px;
        width:40px;
        height:40px;
        text-align:center;
        cursor:pointer;
    }
    #leaderboard{
/*
        border-left: 2px solid #f28a3c;
        border-bottom: 2px solid #f28a3c;
        border-right: 2px solid #f28a3c;
*/
    }

.profilecard{
    background: #f3f8fb;
    display: inline-block;
    padding: 10px;
    border-radius: 5px;
    position:relative;
    width:80%;
    min-height:125px;
}
.ranknum{
    position: absolute;
    top: -10px;
    right: -10px;
    background: #f9ca47;
    padding: 2px 7px;
    border-radius: 15px;
    font-weight: bold;
}

 
</style>


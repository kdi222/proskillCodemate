<table class="generaltable">
	<thead>
		<tr>
			<th class="header c0" style="" scope="col">{{#str}}coursefullname,block_myprogress{{/str}}</th>
			<th class="header c1" style="" scope="col">{{#str}}enrollmentddate,block_myprogress{{/str}}</th>
			<th class="header c2" style="" scope="col">{{#str}}timestarted,block_myprogress{{/str}}</th>
			<th class="header c3 lastcol" style="" scope="col">{{#str}}completiondate,block_myprogress{{/str}}</th>
		</tr>
	</thead>
	<tbody class="empty">
		{{#course_listing}}
		<tr class="">
			<td class="cell c0" style=""><a target="_blank" href="{{{course_link}}}">{{{course_name}}}</a></td>
			<td class="cell c1" style="">{{{enrollment_date}}}</td>
			<td class="cell c2" style="">{{{started_time}}}</td>
			<td class="cell c3 lastcol" style="">{{{completed_time}}}</td>
		</tr>
		{{/course_listing}}
	</tbody>
</table>